<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true ) do | f | %>
<div>
  <%= f.label :"Eメールアドレス:" %>
  <%= f.email_field :email %>
</div>
<div>
  <%= f.label :"パスワード:" %>
  <%= f.password_field :password %>
</div>
<div>
  <%= f.submit "ログイン" %>
</div>
<div>
  <%= link_to "ブログ一覧へ", blogs_path %>
</div>
<% end %>
